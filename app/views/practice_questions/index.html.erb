<div class="page-header">
  <h1>
    Practice questions
    <a href="/practice_questions/new" class="btn btn-lg btn-success">New Practice question</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Response</th>
        <th>Practice interview</th>
        <th>Question</th>
        <th>Actions</th>
      </tr>

      <% @practice_questions.each do |practice_question| %>
      <tr>
        <td><%= practice_question.response %></td>
        <td><%= practice_question.practice_interview_id %></td>
        <td><%= practice_question.question_id %></td>
        <td>
          <a href="/practice_questions/<%= practice_question.id %>" class="btn btn-primary">Show</a>
          <a href="/practice_questions/<%= practice_question.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_practice_question/<%= practice_question.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

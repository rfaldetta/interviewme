<div class="page-header">
  <h1>
    Response lengths
    <a href="/response_lengths/new" class="btn btn-lg btn-success">New Response length</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @response_lengths.each do |response_length| %>
      <tr>
        <td><%= response_length.name %></td>
        <td>
          <a href="/response_lengths/<%= response_length.id %>" class="btn btn-primary">Show</a>
          <a href="/response_lengths/<%= response_length.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_response_length/<%= response_length.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

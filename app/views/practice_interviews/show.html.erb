<div class="page-header">
  <h3>Practice interview created at <%= @practice_interview.created_at %></h3>
</div>

<div class="row">
  <div class="col-md-12">

    <table class="table table-striped table-hover">
      <tr>
        <th>Question</th>
        <th>Your response</th>
      </tr>

      <tr>
        <td><%= @practice_interview.questions.first.question %></td>
        <td><%= @practice_interview.practice_questions.first.response %></td>
      </tr>
      <tr>
      <td><%= @practice_interview.questions.second.question %></td>
        <td><%= @practice_interview.practice_questions.second.response %></td>
      </tr>
      <tr>
        <td><%= @practice_interview.questions.third.question %></td>
        <td><%= @practice_interview.practice_questions.third.response %></td>
      </tr>
      <tr>
        <td><%= @practice_interview.questions.fourth.question %></td>
        <td><%= @practice_interview.practice_questions.fourth.response %></td>
      </tr>
      <tr>
        <td><%= @practice_interview.questions.fifth.question %></td>
        <td><%= @practice_interview.practice_questions.fifth.response %></td>

      </tr>
    </table>


    <div class="btn-group btn-group-justified">
      <a href="/practice_interviews" class="btn btn-primary">
        Back
      </a>
      <a href="/practice_interviews/<%= @practice_interview.id %>/edit" class="btn btn-warning">
        Take interview again
      </a>
      <a href="/delete_practice_interview/<%= @practice_interview.id %>" class="btn btn-danger" rel="nofollow">
        Delete this practice interview
      </a>
    </div>
  </div>
</div>

<div class="page-header">
  <h1>
    Practice interviews
    <a href="/practice_interviews/new" class="btn btn-lg btn-success">New Practice interview</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Actions</th>
      </tr>

      <% @practice_interviews.each do |practice_interview| %>
      <tr>
        <td><%= practice_interview.user_id %></td>
        <td>
          <a href="/practice_interviews/<%= practice_interview.id %>" class="btn btn-primary">Show</a>
          <a href="/practice_interviews/<%= practice_interview.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_practice_interview/<%= practice_interview.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

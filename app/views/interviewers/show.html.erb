<div class="page-header">
  <h3>Interviewer #<%= @interviewer.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @interviewer.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/interviewers" class="btn btn-primary">
        Back
      </a>
      <a href="/interviewers/<%= @interviewer.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_interviewer/<%= @interviewer.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
